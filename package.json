{
  "name": "stjude-cloud-theme",
  "version": "1.0.0",
  "description": "St. Jude Cloud Theme and Guidelines",
  "main": "index.js",
  "scripts": {
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/styles.scss dist/css/style.css",
    "css-minify": "cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/style.min.css dist/css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/stjude/stjde-cloud-theme.git"
  },
  "keywords": [
    "St. Jude",
    "St. Jude Cloud"
  ],
  "author": "",
  "homepage": "https://github.com/stjude/stjde-cloud-theme#readme",
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.12.0"
  }
}
